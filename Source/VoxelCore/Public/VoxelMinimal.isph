FORCEINLINE float SmoothStep(const float A, const float B, const float Alpha)
{
	if (Alpha < A)
	{
		return 0.f;
	}
	else if (Alpha >= B)
	{
		return 1.f;
	}

	const float InterpFraction = (Alpha - A) / (B - A);
	return InterpFraction * InterpFraction * (3.0f - 2.0f * InterpFraction);
}

FORCEINLINE uint32 MurmurHash32(uint32 Hash)
{
	Hash ^= Hash >> 16;
	Hash *= 0x85ebca6b;
	Hash ^= Hash >> 13;
	Hash *= 0xc2b2ae35;
	Hash ^= Hash >> 16;
	return Hash;
}

FORCEINLINE uint32 MurmurHash32(const uint32 Value, const uint32 Seed)
{
	uint32 H = 1831214719 * (1460481823 + Seed);
	return MurmurHash32(H ^ Value);
}

FORCEINLINE bool IsPowerOfTwo(const uint32 Value)
{
	return (Value & (Value - 1)) == 0;
}

FORCEINLINE uniform int32 DivideCeil(const uniform int32 Dividend, const uniform int32 Divisor)
{
	return (Dividend > 0) ? 1 + (Dividend - 1) / Divisor : (Dividend / Divisor);
}
FORCEINLINE uniform int32 DivideFloor(const uniform int32 Dividend, const uniform int32 Divisor)
{
	return Dividend / Divisor;
}

