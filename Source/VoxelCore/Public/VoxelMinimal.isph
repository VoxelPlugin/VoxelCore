FORCEINLINE uint8 FloatToUINT8(const float Float)
{
	return clamp((int32)floor(Float * 255.999f), 0, 255);
}
FORCEINLINE float UINT8ToFloat(const uint8 Int)
{
	return Int / 255.f;
